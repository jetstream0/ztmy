{
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devPath": "http://localhost:5173",
    "distDir": "../build",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "ztmy",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "protocol": {
        "asset": true,
        "assetScope": ["$AUDIO/*"]
      },
      "path": {
        "all": true
      }
    },
    "cli": {
      "subcommands": {
        "concerts": {
          "description": "",
          "args": [
            {
              "name": "concert",
              "index": 1,
              "takesValue": true,
              "required": true
            },
            {
              "name": "video",
              "short": "v",
              "takesValue": true
            }
          ]
        },
        "songs": {
          "args": [
            {
              "name": "song",
              "index": 1,
              "takesValue": true,
              "required": true
            }
          ]
        }
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "dev.prussia.ztmy",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 730,
        "resizable": true,
        "title": "ztmy",
        "width": 1142
      }
    ]
  }
}
